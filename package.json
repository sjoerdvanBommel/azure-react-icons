{
  "name": "@threeveloper/azure-react-icons",
  "description": "A zero-dependency library to get all most recent Azure icons as react components with clear names & full TypeScript support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "bugs": {
    "url": "https://github.com/sjoerdvanBommel/azure-react-icons/issues"
  },
  "repository": "github:sjoerdvanBommel/azure-react-icons",
  "scripts": {
    "build": "npm run generate-azure-icon-components && tsc",
    "generate-azure-icon-components": "tsx src/index.ts src/generated",
    "test": "vitest",
    "test:run": "vitest run",
    "coverage": "vitest run --coverage"
  },
  "keywords": [
    "svg",
    "react",
    "icons",
    "azure",
    "typescript",
    "component"
  ],
  "author": "Sjoerd van Bommel",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^11.0.0",
    "@semantic-release/npm": "^12.0.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.0.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.0.0",
    "@types/react": "^18",
    "@types/unzipper": "^0.10.10",
    "commander": "^11.0.0",
    "jsdom": "^25.0.1",
    "node-fetch": "^3.3.2",
    "react": "^18",
    "react-dom": "^18.3.1",
    "semantic-release": "^24.2.0",
    "svgo": "^3.3.2",
    "tsx": "^4.19.2",
    "typescript": "^5.0.0",
    "unzipper": "^0.12.3",
    "vitest": "^2.1.4"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "version": "1.1.0"
}
